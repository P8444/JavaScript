var eligibilty=function(gradScore, hscScore, sscScore, candidateName){
var gradScore;
var hscScore;
var sscScore;
var candidateName;
var result=gradScore>=70 || hscScore>=80 || sscScore>=90? `Congrats! ${candidateName} You are eligible for TCS interview` :`Unfortunately ${candidateName} You are eligible for TCS interview`
console.log(`${result}`);
}
eligibilty(80, 86, 90, `Poonam`);
eligibilty(70, 65, 55, `Pooja`);
eligibilty(60, 79, 88, `Poonit`);
